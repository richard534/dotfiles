source ~/.intellimacs/spacemacs.vim

" (Optional) Enable other Intellimacs modules
source ~/.intellimacs/extra.vim
source ~/.intellimacs/major.vim
source ~/.intellimacs/hybrid.vim

" (Optional) Enable which-key plugin
source ~/.intellimacs/which-key.vim

"" Personal keybinds

" Find file in project
" nnoremap <leader>fp    :action GotoFile<CR>

" Search in current file by using Intellij Search
" nnoremap <leader>ss    :action Find<CR>

" imap jk <Esc>
" set timeoutlen=1000

" set clipboard+=unnamed

" enable vim commenting
" set commentary
